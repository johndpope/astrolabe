							</Begin>
							<End>
								<Angle>
									<Rational value="359.9999"/>
								</Angle>
							</End>
							<Angle>
								<Rational value="-19.9"/>
							</Angle>
						</CircleParallel>
					</Circle>
				</HorizonEquatorial>
			</Horizon>
			<!--Ã¤quatorial-->
