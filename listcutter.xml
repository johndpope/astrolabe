					<AnnotationStraight reverse="false" radiant="false" anchor="topleft">
						<Script purpose="none" value="astrolabe"/>
						<Frame number="1" anchor="topleft"/>
					</AnnotationStraight>
					<!--planets-->
					<Body>
						<BodyPlanet type="mars" nature="celestial">
							<Annotation>
								<AnnotationCurved reverse="true" anchor="bottommiddle" distance="97.8">
									<Script purpose="diallettering" value="Mars"/>
								</AnnotationCurved>
							</Annotation>
							<DialDay reflect="true" baseline="none">
								<GraduationSpan value="10" division="1">
									<AnnotationStraight reverse="false" anchor="middleleft" radiant="false">
										<Script purpose="guidance" value="{T}. {monatkurz}"/>
									</AnnotationStraight>
								</GraduationSpan>
							</DialDay>
						</BodyPlanet>
					</Body>
