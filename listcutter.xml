				<HorizonEquatorial practicality="always">
					<AnnotationStraight reverse="false" radiant="false" anchor="topleft">
						<Script purpose="none" value="astrolabe"/>
						<Frame number="1" anchor="topleft"/>
					</AnnotationStraight>
					<Circle>
						<CircleParallel importance="canonical">
							<Begin>
								<Angle>
									<Rational value="0.0"/>
